#include "HashApp.h"

HashApp::HashApp(){
	hashTable = new HashFile();
	setMainMenu(new MainMenu(hashTable));
}

HashTable* HashApp::getHash(){
	return hashTable;
}

void HashApp::setHashTable(HashFile *hashTable){
	this->hashTable = hashTable;
}

void HashApp::release(){
	Application::release();

	if(hashTable!=NULL){
		hashTable->release();		
	}		
	delete hashTable;
}
